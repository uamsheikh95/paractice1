<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="1">
        <record id="sequence_saving_account" model="ir.sequence">
            <field name="name">Saving Account Sequence</field>
            <field name="code">saving.account.seq</field>
            <field name="prefix">ACC/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field name="padding">6</field>
        </record>
        <record id="sequence_saving_transaction" model="ir.sequence">
            <field name="name">Saving Account Transaction</field>
            <field name="code">saving.transaction.seq</field>
            <field name="prefix">TR/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field name="padding">6</field>
        </record>
    </data>
</odoo>