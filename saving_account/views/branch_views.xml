<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <!-- Form View -->
    <record id="branch_view_form" model="ir.ui.view">
        <field name="name">saving_account.branch.form</field>
        <field name="model">saving_account.branch</field>
        <field name="arch" type="xml">
            <form string="Saving Type Form">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                        <group>
                            
                        </group>
                    </group>
                </sheet>
                <chatter/>
            </form>
        </field>
    </record>
    <!-- List View -->
    <record id="branch_view_list" model="ir.ui.view">
        <field name="name">saving_account.branch.list</field>
        <field name="model">saving_account.branch</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>
    <!-- Actions -->
    <record model="ir.actions.act_window" id="branch_action">
        <field name="name">Branches</field>
        <field name="res_model">saving_account.branch</field>
        <field name="view_mode">list,form</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                    Click to create a new branch.
                </p>
        </field>
    </record>
</odoo>